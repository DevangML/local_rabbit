# API Configuration
# If not specified, the client will auto-detect the server
# VITE_API_BASE_URL=http://localhost:3001
# VITE_API_PORT=3001

# Client Configuration
# If not specified, the client will default to port 3000
# VITE_CLIENT_PORT=3000

# Environment
VITE_NODE_ENV=development

# Feature Flags
VITE_ENABLE_ANALYTICS=false
VITE_ENABLE_THEMES=true
VITE_ENABLE_AI_FEATURES=true

# AI Configuration
# Get your API key from https://ai.google.dev/ (Google AI Studio)
VITE_GEMINI_API_KEY=your-gemini-api-key-here 