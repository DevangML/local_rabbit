{
  "name": "@local-rabbit/server",
  "version": "1.0.0",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "node src/server.js",
    "prebuild": "rimraf dist && yarn workspace @local-rabbit/client build",
    "build": "node scripts/build.js",
    "build:prod": "rimraf dist && yarn workspace @local-rabbit/client build:prod && node scripts/pre-build-prod.js",
    "start": "node src/server.js",
    "start:prod": "NODE_ENV=production node dist/server.js",
    "start:optimized": "node scripts/start-optimized.js",
    "install:perf-deps": "node scripts/install-performance-deps.js",
    "clean": "rimraf dist",
    "lint": "eslint . --ext .js || true",
    "lint:fix": "eslint . --ext .js --fix || true",
    "lint:quiet": "eslint . --ext .js --quiet || true",
    "test": "jest",
    "fix:code-quality": "./fix-code-quality.sh"
  },
  "dependencies": {
    "@babel/runtime": "^7.26.10",
    "@local-rabbit/shared": "^1.0.0",
    "@vitejs/plugin-react": "^4.2.1",
    "compression": "^1.7.4",
    "cors": "^2.8.5",
    "dotenv": "^16.4.7",
    "express": "^4.21.2",
    "express-rate-limit": "^7.1.5",
    "helmet": "^7.2.0",
    "morgan": "^1.10.0",
    "react": "^19.0.0",
    "react-dom": "^19.0.0",
    "react-router-dom": "^6.30.0",
    "sirv": "^2.0.4",
    "winston": "^3.17.0"
  },
  "devDependencies": {
    "@babel/cli": "^7.26.4",
    "@babel/core": "^7.26.9",
    "@babel/plugin-transform-runtime": "^7.24.0",
    "@babel/preset-env": "^7.26.9",
    "@babel/preset-react": "^7.26.3",
    "eslint": "9.21.0",
    "eslint-plugin-security": "3.0.1",
    "nodemon": "^3.1.9",
    "vite": "^5.1.3",
    "vitest": "^1.2.2"
  }
}