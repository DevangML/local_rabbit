{
  "name": "@local-rabbit/server",
  "version": "1.0.0",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "nodemon --exec ts-node src/server.ts",
    "prebuild": "rimraf dist && yarn workspace @local-rabbit/client build",
    "build": "rimraf dist && yarn workspace @local-rabbit/client build && tsc --skipLibCheck || true && babel src --out-dir dist --extensions '.ts,.tsx' --copy-files",
    "start": "node dist/server.js",
    "clean": "rimraf dist",
    "lint": "eslint --config .eslintrc.cjs . --ext .ts,.js --rule 'no-undef: off'",
    "lint:fix": "eslint --config .eslintrc.cjs . --ext .ts,.js --fix --rule 'no-undef: off'",
    "lint:quiet": "eslint --config .eslintrc.cjs . --ext .ts,.js --quiet --rule 'no-undef: off' || true",
    "test": "jest"
  },
  "dependencies": {
    "@babel/runtime": "^7.24.0",
    "@local-rabbit/shared": "^1.0.0",
    "@vitejs/plugin-react": "^4.2.1",
    "compression": "^1.7.4",
    "cors": "^2.8.5",
    "dotenv": "^16.4.7",
    "express": "^4.21.2",
    "helmet": "^7.2.0",
    "morgan": "^1.10.0",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-router-dom": "^6.30.0",
    "sirv": "^2.0.4"
  },
  "devDependencies": {
    "@babel/cli": "^7.26.4",
    "@babel/core": "^7.26.9",
    "@babel/plugin-transform-runtime": "^7.24.0",
    "@babel/preset-env": "^7.26.9",
    "@babel/preset-react": "^7.26.3",
    "@babel/preset-typescript": "^7.26.0",
    "@types/compression": "^1.7.5",
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/morgan": "^1.9.9",
    "@types/node": "^20.11.19",
    "@types/react": "^18.2.55",
    "@types/react-dom": "^18.2.19",
    "@typescript-eslint/eslint-plugin": "^6.21.0",
    "@typescript-eslint/parser": "^6.21.0",
    "eslint": "^8.57.1",
    "tsx": "^4.7.1",
    "typescript": "^5.3.3",
    "vite": "^5.1.3",
    "vitest": "^1.2.2"
  }
}