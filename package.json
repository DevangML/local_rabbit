{
  "name": "local_rabbit",
  "private": true,
  "workspaces": [
    "packages/*"
  ],
  "scripts": {
    "dev": "npm run build --workspace=@local-rabbit/client && concurrently \"npm run dev --workspace=@local-rabbit/server\" \"npm run dev --workspace=@local-rabbit/client\"",
    "build": "npm run build -ws --if-present",
    "start": "node packages/server/dist/server.js",
    "test": "npm run test -ws --if-present",
    "lint": "npm run lint -ws --if-present",
    "clean": "npm run clean -ws --if-present && rm -rf node_modules"
  },
  "devDependencies": {
    "concurrently": "^9.1.2",
    "nodemon": "^3.1.9"
  },
  "packageManager": "yarn@1.22.22",
  "engines": {
    "node": ">=18.0.0",
    "yarn": ">=1.22.0"
  },
  "dependencies": {
    "cors": "^2.8.5",
    "express": "^4.21.2",
    "morgan": "^1.10.0",
    "prismjs": "^1.29.0",
    "react-markdown": "^10.0.1",
    "remark-gfm": "^4.0.1"
  }
}