{
  "name": "local-rabbit",
  "version": "1.0.0",
  "type": "module",
  "private": true,
  "workspaces": [
    "packages/*"
  ],
  "scripts": {
    "build:client": "cd packages/client && yarn run vite build",
    "build:client:ssr": "cd packages/client && yarn run vite build --ssr src/entry-server.jsx --outDir dist/server",
    "build:server": "cd packages/server && yarn build",
    "build": "yarn build:client && yarn build:client:ssr && yarn build:server",
    "dev:client": "cd packages/client && yarn run vite build --watch",
    "dev:server": "cd packages/server && jsx watch src/server.js",
    "dev": "concurrently \"yarn dev:client\" \"yarn dev:server\"",
    "start": "cd packages/server && yarn start",
    "clean": "rimraf packages/*/dist",
    "lint": "eslint --config eslint.config.js . --ext .js, --fix",
    "lint:check": "eslint --config eslint.config.js . --ext .js",
    "lint:fix": "yarn workspaces run lint:fix",
    "lint:quiet": "yarn workspaces run lint:quiet",
    "fix:code-quality": "./fix-all-code-quality.sh",
    "fix:code-quality:client": "cd packages/client && yarn fix:code-quality",
    "fix:code-quality:server": "cd packages/server && yarn fix:code-quality"
  },
  "devDependencies": {
    "@babel/eslint-parser": "^7.26.8",
    "@babel/preset-react": "^7.26.3",
    "@eslint/js": "^9.21.0",
    "@alloc/quick-lru": "5.2.0",
    "concurrently": "^8.2.2",
    "eslint": "^9.21.0",
    "eslint-plugin-babel": "^5.3.1",
    "eslint-plugin-react": "^7.37.4",
    "eslint-plugin-security": "^3.0.1",
    "globals": "^14.0.0",
    "rimraf": "^5.0.5",
    "rollup": "3.29.5",
    "@rollup/plugin-commonjs": "25.0.7",
    "@rollup/plugin-node-resolve": "15.2.3"
  },
  "packageManager": "yarn@1.22.22",
  "engines": {
    "node": ">=18.0.0",
    "yarn": ">=1.22.0"
  },
  "dependencies": {
    "@emotion/cache": "^11.14.0",
    "@emotion/react": "^11.14.0",
    "@emotion/styled": "^11.14.0",
    "@google/generative-ai": "^0.24.0",
    "@mui/styled-engine": "^6.4.6",
    "cors": "^2.8.5",
    "express": "^4.21.2",
    "morgan": "^1.10.0",
    "prismjs": "^1.29.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-markdown": "^10.0.1",
    "remark-gfm": "^4.0.1",
    "simple-git": "^3.27.0"
  },
  "resolutions": {
    "@remix-run/router": "1.13.1",
    "react-router": "6.22.1",
    "react-router-dom": "6.22.1",
    "@alloc/quick-lru": "5.2.0",
    "rollup": "3.29.5",
    "@rollup/plugin-commonjs": "25.0.7",
    "@rollup/plugin-node-resolve": "15.2.3",
    "rollup-plugin-typescript2": "0.36.0",
    "rollup-plugin-dts": "6.1.0",
    "@rollup/pluginutils": "5.0.5",
    "any-promise": "1.3.0",
    "vite": "4.5.2",
    "@vitejs/plugin-react": "4.2.1",
    "@types/node": "18.19.3",
    "esbuild": "0.18.20",
    "@esbuild/darwin-x64": "0.18.20",
    "@esbuild/linux-x64": "0.18.20",
    "@esbuild/win32-x64": "0.18.20"
  }
}